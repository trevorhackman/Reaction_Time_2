<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke android:color="@color/table_border" android:width="@dimen/tableStrokeWidth" />
            <solid android:color="@color/table_background"/>
            <corners android:topLeftRadius="@dimen/tableCornerRadius" android:topRightRadius="@dimen/tableCornerRadius" />
        </shape>
    </item>
    <item android:left="@dimen/tableStrokeWidth" android:right="@dimen/tableStrokeWidth" android:top="@dimen/tableStrokeWidth">
        <shape android:shape="rectangle">
            <solid android:color="@color/table_background" />
            <corners android:topLeftRadius="@dimen/tableCornerRadius" android:topRightRadius="@dimen/tableCornerRadius" />
        </shape>
    </item>
</layer-list>