<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/table_row_item_background">
        <shape android:shape="rectangle">
            <solid android:color="@color/table_row"/>
            <stroke android:color="@color/table_row_stroke" android:width="@dimen/tableRowStrokeWidth"/>
        </shape>
    </item>
    <item android:left="@dimen/tableRowStrokeWidth" android:right="@dimen/tableRowStrokeWidth">
        <shape android:shape="rectangle">
            <solid android:color="@color/table_row" />
        </shape>
    </item>
</layer-list>